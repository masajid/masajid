<%= simple_form_for @social_network, url: social_network_path, method: :put, wrapper: :horizontal_form do |f| %>
  <%= f.error_notification %>

  <%= f.input :youtube %>
  <%= f.input :facebook %>
  <%= f.input :google %>
  <%= f.input :twitter %>
  <%= f.input :vimeo %>
  <%= f.input :instagram %>

  <%= f.button :submit, class: 'btn btn-info pull-right' %>
<% end %>
